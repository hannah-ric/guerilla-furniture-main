{
    "name": "blueprint-buddy",
    "private": true,
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 200",
        "preview": "vite preview",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "typecheck": "tsc --noEmit",
        "clean": "rimraf dist",
        "prebuild": "npm run clean && npm run typecheck",
        "setup": "bash scripts/setup.sh",
        "setup:codex": "bash scripts/codex-env.sh",
        "verify": "npm run typecheck && npm run lint && npm run build",
        "verify:full": "bash scripts/verify-setup.sh",
        "test:env": "bash scripts/quick-test.sh",
        "supabase:start": "supabase start",
        "supabase:stop": "supabase stop",
        "supabase:db-push": "supabase db push",
        "supabase:gen-types": "supabase gen types typescript --local > src/lib/database.types.ts"
    },
    "dependencies": {
        "@langchain/community": "^0.3.45",
        "@radix-ui/react-dialog": "^1.1.2",
        "@radix-ui/react-dropdown-menu": "^2.1.2",
        "@radix-ui/react-label": "^2.1.0",
        "@radix-ui/react-slot": "^1.1.0",
        "@radix-ui/react-tabs": "^1.1.1",
        "@radix-ui/react-toast": "^1.2.2",
        "@react-three/drei": "^9.114.0",
        "@react-three/fiber": "^8.17.10",
        "@supabase/supabase-js": "^2.46.1",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.1",
        "langchain": "^0.3.27",
        "lucide-react": "^0.451.0",
        "openai": "^4.67.3",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.53.0",
        "react-router-dom": "^6.28.0",
        "tailwind-merge": "^2.5.4",
        "tailwindcss-animate": "^1.0.7",
        "three": "^0.169.0",
        "three-mesh-bvh": "^0.8.0",
        "zod": "^3.23.8",
        "zustand": "^5.0.1"
    },
    "devDependencies": {
        "@types/node": "^22.8.6",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@types/three": "^0.169.0",
        "@typescript-eslint/eslint-plugin": "^8.12.2",
        "@typescript-eslint/parser": "^8.12.2",
        "@vitejs/plugin-react": "^4.3.3",
        "autoprefixer": "^10.4.20",
        "eslint": "^9.13.0",
        "eslint-plugin-react-hooks": "^5.0.0",
        "eslint-plugin-react-refresh": "^0.4.13",
        "globals": "^16.2.0",
        "postcss": "^8.4.47",
        "rimraf": "^6.0.1",
        "supabase": "^1.200.3",
        "tailwindcss": "^3.4.14",
        "typescript": "^5.6.3",
        "vite": "^6.0.1",
        "vitest": "^2.1.4"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    }
}
